{
  "port": 8501,
  "host": "localhost",
  "debug": false,
  "log_level": "INFO",
  "generated_diagrams_dir": "generated-diagrams",
  "logs_dir": "logs",
  "test_screenshots_dir": "test_screenshots",
  "agent_timeout": 120,
  "max_diagrams": 100,
  "cleanup_old_diagrams": true,
  "max_diagram_age_hours": 24,
  "streamlit_config": {
    "server.headless": true,
    "browser.gatherUsageStats": false,
    "server.enableCORS": false,
    "server.enableXsrfProtection": false
  },
  "agent_config": {
    "model_temperature": 0.7,
    "max_tokens": 4000,
    "timeout_seconds": 120
  },
  "diagram_config": {
    "auto_cleanup": true,
    "max_file_size_mb": 10,
    "supported_formats": ["png", "jpg", "jpeg"]
  },
  "logging_config": {
    "log_to_file": true,
    "log_to_console": true,
    "max_log_size_mb": 50,
    "backup_count": 5
  }
}